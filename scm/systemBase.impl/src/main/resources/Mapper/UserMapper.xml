<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.system.systembase.impl.apiTest.mapper.UserMapper">

    <resultMap id="baseResultMap" type="com.system.systembase.api.apiTest.domain.User">
        <result column="id" property="id"/>
        <result column="createUserName" property="createUserName"/>
        <result column="createUserUid" property="createUserUid"/>
        <result column="contactsUser" property="contactsUser"/>
        <result column="contactsUserPhone" property="contactsUserPhone"/>
    </resultMap>

    <select id="selectByIdInXML" resultType="com.system.systembase.api.apiTest.domain.User">
        select *
        from scm_sys_user
        where id = #{param.id}
        limit 1;
    </select>
</mapper>